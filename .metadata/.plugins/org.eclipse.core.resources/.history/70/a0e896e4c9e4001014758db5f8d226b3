#ifndef INC_TYPES_H_
#define INC_TYPES_H_

/* definitions of some structures of date and time */
typedef struct
{
	uint8_t date;
	uint8_t month;
	uint8_t	year;
	uint8_t day;
}RTC_date_t;

typedef struct
{
	uint8_t seconds;
	uint8_t minutes;
	uint8_t	hours;
	uint8_t time_format;
}RTC_time_t;

typedef struct
{
	double temperature;
	double pressure;
	double humidity;

}BME280_data_t;

typedef struct
{
	char timestamp[17];

};

#endif /* INC_TYPES_H_ */
